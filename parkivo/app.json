{
  "expo": {
    "name": "parkivo",
    "slug": "parkivo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "parkivo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCeXQMardrYV9PtExu0ev8VkY72bNlbVKE"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "package": "com.yourname.parkivo",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCeXQMardrYV9PtExu0ev8VkY72bNlbVKE"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "config": {
        "googleMapsApiKey": "AIzaSyCeXQMardrYV9PtExu0ev8VkY72bNlbVKE"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Parkivo to use your location to find nearby parking spots."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/parkSplash.avif",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
