{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./node_modules/expo/tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
    "**/*.js",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}